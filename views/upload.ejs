<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Upload Files</title>
</head>
<body>
    <h2>uploaded</h2>
    <a href="/">Home</a>
    <% text.forEach((sentence, index) => { %>
        <div class="upload-item"><img src="<%= images[index] %>" alt=""><p>&nbsp;&nbsp;<%= index + 1 %>: <%= sentence %></p></div>
    <% }); %>

    <a href="/app/create">Create Video</a>
</body>
</html>